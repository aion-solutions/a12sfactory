        "drupal/block_field": "^1.0@RC",
        "drupal/ckeditor_media_embed": "~1.7",
        "drupal/components": "~2.0-beta2",
        "drupal/config_ignore": "~2.2",
        "drupal/config_perms": "~2.0",
