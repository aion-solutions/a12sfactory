{
    "name": "aion-solutions/a12sfactory",
    "description": "A Website factory built by Aïon Solutions",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Aïon Solutions",
            "homepage": "https://www.aion-solutions.lu",
            "role": "Maintainer"
        }
    ],
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "assets": {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    },
    "require": {
        "composer/installers": "^1.6.0",
        "cweagans/composer-patches": "^1.6",
